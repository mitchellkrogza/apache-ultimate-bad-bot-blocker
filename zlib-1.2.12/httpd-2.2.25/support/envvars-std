if test "x$LD_LIBRARY_PATH" != "x" ; then
  LD_LIBRARY_PATH="/usr/local/apache2/lib:$LD_LIBRARY_PATH"
else
  LD_LIBRARY_PATH="/usr/local/apache2/lib"
fi
export LD_LIBRARY_PATH
